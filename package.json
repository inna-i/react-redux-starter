{
  "name": "my-app",
  "version": "1.0.0",
  "description": "My App",
  "license": "Apache-2.0",
  "author": "Inna Ivashchuk innes333@gmail.com",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "roots": [
      "src"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/scripts/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "github:Talend/platform-services-portal"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "6.4.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.16.0",
    "compression-webpack-plugin": "0.4.0",
    "css-loader": "0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "i18next-parser": "^0.12.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.3",
    "node-sass": "4.7.2",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "1.2.3",
    "resolve-url-loader": "2.0.2",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "0.4.6",
    "underscore-template-loader": "^0.8.0",
    "webpack": "3.6.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "2.9.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.5",
    "copy-webpack-plugin": "^4.4.1",
    "i18next": "^10.0.1",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.4.3",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "lodash.capitalize": "4.2.1",
    "@material-ui/core": "3.9.2",
    "moment-timezone": "^0.5.15",
    "prop-types": "15.5.10",
    "react": "^15.6.2",
    "react-ace": "^5.4.0",
    "react-addons-css-transition-group": "15.5.2",
    "react-bootstrap": "0.31.0",
    "react-css-transition": "^0.7.4",
    "react-dom": "^15.6.2",
    "react-i18next": "^6.0.6",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-transition-group": "1.2.0",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "4.2.1",
    "redux-thunk": "2.2.0",
    "save-dev": "^2.0.0",
    "urijs": "1.18.10",
    "url-loader": "0.6.2"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "lint-es": "eslint --config .eslintrc src/**/*",
    "lint-sass": "sass-lint -q -v",
    "lint": "yarn run lint-es && yarn run lint-sass",
    "start": "node devServer/mockserver.js",
    "debug": "NODE_ENV=production node $NODE_DEBUG_OPTION ./node_modules/.bin/webpack-dev-server",
    "test": "jest --config .jestrc",
    "test:watch": "jest --config .jestrc --watch",
    "test:cov": "jest --config .jestrc --coverage",
    "webpack-prod": "NODE_ENV=production webpack",
    "webpack": "NODE_ENV=development webpack",
    "extract-i18n": "npm run extract-i18n-app",
    "extract-i18n-app": "i18next src/ --fileFilter \"*.js\" -r -o src/locales/locale-app -n locale-app -l en,fr,ja --write-old false --ignore-variables"
  }
}
